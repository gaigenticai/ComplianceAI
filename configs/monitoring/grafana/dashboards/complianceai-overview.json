{
  "dashboard": {
    "id": null,
    "title": "ComplianceAI System Overview",
    "tags": ["complianceai", "overview"],
    "timezone": "UTC",
    "panels": [
      {
        "id": 1,
        "title": "System Health Status",
        "type": "stat",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "up",
            "legendFormat": "{{ job }}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN",
                    "color": "red"
                  },
                  "1": {
                    "text": "UP",
                    "color": "green"
                  }
                },
                "type": "value"
              }
            ]
          }
        }
      },
      {
        "id": 2,
        "title": "Active Alerts",
        "type": "table",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "targets": [
          {
            "expr": "ALERTS{alertstate=\"firing\"}",
            "legendFormat": "{{ alertname }}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "displayMode": "color-text"
            }
          }
        }
      },
      {
        "id": 3,
        "title": "CPU Usage",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "targets": [
          {
            "expr": "100 - (avg by(instance) (irate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
            "legendFormat": "{{ instance }}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "format": "percent",
            "label": "CPU Usage %"
          }
        ]
      },
      {
        "id": 4,
        "title": "Memory Usage",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "targets": [
          {
            "expr": "(1 - node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes) * 100",
            "legendFormat": "{{ instance }}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "format": "percent",
            "label": "Memory Usage %"
          }
        ]
      },
      {
        "id": 5,
        "title": "HTTP Request Rate",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        },
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{ job }} - {{ method }} {{ status }}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "format": "reqps",
            "label": "Requests per Second"
          }
        ]
      },
      {
        "id": 6,
        "title": "HTTP Response Time",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile - {{ job }}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile - {{ job }}",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "format": "s",
            "label": "Response Time (seconds)"
          }
        ]
      },
      {
        "id": 7,
        "title": "Database Connections",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        },
        "targets": [
          {
            "expr": "pg_stat_activity_count",
            "legendFormat": "PostgreSQL Active Connections",
            "refId": "A"
          },
          {
            "expr": "mongodb_connections{state=\"active\"}",
            "legendFormat": "MongoDB Active Connections",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "format": "short",
            "label": "Connection Count"
          }
        ]
      },
      {
        "id": 8,
        "title": "Disk Usage",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 24
        },
        "targets": [
          {
            "expr": "(node_filesystem_size_bytes - node_filesystem_avail_bytes) / node_filesystem_size_bytes * 100",
            "legendFormat": "{{ instance }} - {{ mountpoint }}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "format": "percent",
            "label": "Disk Usage %"
          }
        ]
      },
      {
        "id": 9,
        "title": "Report Generation Status",
        "type": "table",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 32
        },
        "targets": [
          {
            "expr": "complianceai_report_generation_status",
            "legendFormat": "{{ status }}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "pending": {
                    "text": "PENDING",
                    "color": "yellow"
                  },
                  "running": {
                    "text": "RUNNING",
                    "color": "blue"
                  },
                  "completed": {
                    "text": "COMPLETED",
                    "color": "green"
                  },
                  "failed": {
                    "text": "FAILED",
                    "color": "red"
                  }
                },
                "type": "value"
              }
            ]
          }
        }
      },
      {
        "id": 10,
        "title": "Regulatory Deadlines",
        "type": "table",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 32
        },
        "targets": [
          {
            "expr": "complianceai_regulatory_deadline_hours_remaining",
            "legendFormat": "{{ report_type }}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "from": 0,
                  "to": 24,
                  "result": {
                    "text": "URGENT",
                    "color": "red"
                  }
                },
                "type": "range"
              },
              {
                "options": {
                  "from": 24,
                  "to": 168,
                  "result": {
                    "text": "WARNING",
                    "color": "orange"
                  }
                },
                "type": "range"
              },
              {
                "options": {
                  "from": 168,
                  "to": 999999,
                  "result": {
                    "text": "OK",
                    "color": "green"
                  }
                },
                "type": "range"
              }
            ]
          }
        }
      },
      {
        "id": 11,
        "title": "Error Rate by Service",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 40
        },
        "targets": [
          {
            "expr": "rate(http_requests_total{status=~\"5..\"}[5m]) / rate(http_requests_total[5m]) * 100",
            "legendFormat": "{{ job }}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "format": "percent",
            "label": "Error Rate %"
          }
        ]
      },
      {
        "id": 12,
        "title": "Kafka Consumer Lag",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 40
        },
        "targets": [
          {
            "expr": "kafka_consumergroup_lag",
            "legendFormat": "{{ consumergroup }} - {{ topic }}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "format": "short",
            "label": "Lag (messages)"
          }
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "5s",
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ]
    },
    "templating": {
      "list": [
        {
          "current": {
            "selected": false,
            "text": "All",
            "value": "$__all"
          },
          "hide": 0,
          "includeAll": true,
          "label": "Instance",
          "multi": true,
          "name": "instance",
          "options": [],
          "query": "label_values(instance)",
          "refresh": 1,
          "regex": "",
          "sort": 0,
          "tagValuesQuery": "",
          "tags": [],
          "tagsQuery": "",
          "type": "query",
          "useTags": false
        },
        {
          "current": {
            "selected": false,
            "text": "All",
            "value": "$__all"
          },
          "hide": 0,
          "includeAll": true,
          "label": "Job",
          "multi": true,
          "name": "job",
          "options": [],
          "query": "label_values(job)",
          "refresh": 1,
          "regex": "",
          "sort": 0,
          "tagValuesQuery": "",
          "tags": [],
          "tagsQuery": "",
          "type": "query",
          "useTags": false
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        }
      ]
    },
    "editable": true,
    "gnetId": null,
    "graphTooltip": 0,
    "links": [],
    "refresh": "30s",
